<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    >
<data>
    <variable
        name="stocks"
        type="com.diren.veripark.network.responses.Stocks" />
</data>
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/txtSymbol"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="deneme"
            android:layout_marginStart="6dp"
            android:layout_marginEnd="4dp"
            android:layout_marginTop="7dp"
            android:layout_marginBottom="7dp">

        </TextView>

        <TextView
            android:id="@+id/txtPrice"
            android:text="deneme"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_marginTop="7dp"
            android:layout_marginBottom="7dp" />

        <TextView
            android:text="deneme"
            android:id="@+id/txtDifference"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_marginTop="7dp"
            android:layout_marginBottom="7dp" />

        <TextView
            android:text="deneme"
            android:id="@+id/txtVolume"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_marginTop="7dp"
            android:layout_marginBottom="7dp" />
        <TextView
            android:id="@+id/txtReceiving"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:text="deneme"
            android:layout_marginTop="7dp"
            android:layout_marginBottom="7dp" />

        <TextView
            android:id="@+id/txtSale"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:text="deneme"
            android:layout_marginTop="7dp"
            android:layout_marginBottom="7dp" />

        <TextView
            android:id="@+id/txtChange"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:text="deneme"
            android:layout_marginTop="7dp"
            android:layout_marginBottom="7dp" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/txtSymbolData"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="deneme"
            android:layout_marginStart="6dp"
            android:layout_marginEnd="4dp"
            android:layout_marginTop="7dp"
            android:layout_marginBottom="7dp">

        </TextView>

        <TextView
            android:id="@+id/txtPriceData"
            android:text="deneme"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_marginTop="7dp"
            android:layout_marginBottom="7dp" />

        <TextView
            android:text="deneme"
            android:id="@+id/txtDifferenceData"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_marginTop="7dp"
            android:layout_marginBottom="7dp" />

        <TextView
            android:text="deneme"
            android:id="@+id/txtVolumeData"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_marginTop="7dp"
            android:layout_marginBottom="7dp" />
        <TextView
            android:id="@+id/txtReceivingData"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:text="deneme"
            android:layout_marginTop="7dp"
            android:layout_marginBottom="7dp" />

        <TextView
            android:id="@+id/txtSaleData"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:text="deneme"
            android:layout_marginTop="7dp"
            android:layout_marginBottom="7dp" />

        <TextView
            android:id="@+id/txtChangeData"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:text="deneme"
            android:layout_marginTop="7dp"
            android:layout_marginBottom="7dp" />
    </LinearLayout>
</LinearLayout>
</layout>